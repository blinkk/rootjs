{
  "name": "@private/docs",
  "version": "0.0.0",
  "private": true,
  "description": "Documentation site for rootjs.dev",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "root dev",
    "build": "root package --target=firebase --out=functions && touch .env && cp .env functions",
    "preview": "rm -rf dist/ && root build --ssr-only && root preview",
    "deploy": "rm -rf dist/ && pnpm run build && firebase deploy",
    "types": "root-cms types"
  },
  "author": "s@blinkk.com",
  "license": "MIT",
  "dependencies": {
    "@blinkk/root": "workspace:*",
    "@blinkk/root-cms": "workspace:*",
    "@tabler/icons-preact": "2.39.0",
    "highlight.js": "11.6.0",
    "preact": "10.19.3",
    "preact-render-to-string": "6.3.1"
  }
}
