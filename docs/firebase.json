{
  "hosting": {
    "site": "rootjs-dev",
    "public": "functions/dist/html",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "i18n": {
      "root": "/"
    },
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "**",
        "function": "www-server",
        "pinTag": true
      }
    ],
    "functions": {
      "codebase": "www"
    }
  }
}
