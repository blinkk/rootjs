    "test": "pnpm run build",
    "types": "root-cms generate-types"
  },
  "dependencies": {
    "@blinkk/root": "workspace:*",
    "@blinkk/root-cms": "workspace:*",
    "firebase-admin": "11.11.1",
    "preact": "10.27.1",
    "preact-render-to-string": "6.6.1"
  },
  "devDependencies": {
    "@types/node": "18.11.18"
  }
}
