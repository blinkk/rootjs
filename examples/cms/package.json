{
  "name": "@examples/cms",
  "description": "Minimal Root CMS project.",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "root dev",
    "build": "rm -rf dist/ && root build",
    "preview": "rm -rf dist/ out/ && root build --ssr-only && root preview",
