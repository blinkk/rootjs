{
  "name": "@blinkk/root",
  "version": "1.4.10",
  "author": "s@blinkk.com",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinkk/rootjs.git",
    "directory": "packages/root"
  },
  "files": [
    "bin/*",
    "dist/**/*",
    "client.d.ts"
  ],
  "bin": {
    "root": "./bin/root.js"
  },
  "type": "module",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    "./client": "./client.d.ts",
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./functions": {
      "types": "./dist/functions.d.ts",
      "import": "./dist/functions.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    },
    "./render": {
      "types": "./dist/render.d.ts",
      "import": "./dist/render.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsup-node",
    "dev": "nodemon --watch src -e ts,tsx --exec pnpm build",
    "test": "pnpm build && vitest run --test-timeout=30000",
    "test:watch": "pnpm build && vitest"
  },
  "dependencies": {
    "@types/js-yaml": "4.0.9",
    "@types/micromatch": "4.0.6",
    "bundle-require": "4.0.2",
    "busboy": "1.6.0",
    "commander": "11.0.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "dotenv": "16.4.5",
    "esbuild": "0.19.9",
    "express": "4.18.2",
    "fs-extra": "11.1.1",
    "html-minifier-terser": "7.2.0",
    "js-beautify": "1.14.9",
    "js-yaml": "4.1.0",
    "kleur": "4.1.5",
    "micromatch": "4.0.5",
    "sass": "1.69.3",
    "sirv": "2.0.3",
    "source-map-support": "0.5.21",
    "tiny-glob": "0.2.9",
    "vite": "5.0.8",
    "workspace-tools": "0.37.0"
  },
  "peerDependencies": {
    "firebase-admin": ">=11",
    "firebase-functions": ">=4",
    "preact": ">=10",
    "preact-render-to-string": ">=5"
  },
  "peerDependenciesMeta": {
    "firebase-functions": {
      "optional": true
    },
    "firebase-admin": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/busboy": "1.5.0",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/fs-extra": "11.0.2",
    "@types/html-minifier-terser": "7.0.0",
    "@types/js-beautify": "1.14.1",
    "@types/node": "20.8.4",
    "@types/preact-custom-element": "4.0.2",
    "firebase-admin": "11.11.0",
    "firebase-functions": "4.8.0",
    "nodemon": "3.0.1",
    "preact": "10.19.3",
    "preact-custom-element": "4.3.0",
    "preact-render-to-string": "6.3.1",
    "tsup": "8.0.1",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  }
}
